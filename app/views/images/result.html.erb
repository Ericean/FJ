<h1>Listing Results</h1>

<table>
  <thead>
    <tr>
      <th>Character</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |img| %>
      <tr>
        <td><%= image_tag(img.url, :border => 0) %></td>
        <td><%= img.result %></td>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @results %>
<%= link_to 'Back', images_path %> 