	<li>
		<div class="candidate">
		<%= candidate.content %> &nbsp
		<% if current_user.name === "admin" %>
		<%= link_to "delete", [@image, candidate], method: :delete, data: { confirm: 'Are you sure?' }	 %> |
		<% end %>
		<%= link_to image_tag("vote.svg.png", :border => 0), [@image, candidate], method: :put%> |
		<%= candidate.votesum %>
		</div>
	</li>

